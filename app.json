{
  "expo": {
    "name": "Guroo",
    "description": "Making your home life. Simple.",
    "slug": "guroo",
    "scheme": "guroo",
    "version": "1.0.0",
    "privacy": "unlisted",
    "platforms": [
      "ios",
      "android"
    ],
