    "splash": {
      "image": "./assets/images/splash.png",
      "backgroundColor": "#FFFFFF",
      "resizeMode": "cover"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "ios": {
      "bundleIdentifier": "com.guroosolutions.guroo"
    },
    "android": {
      "package": "com.guroosolutions.guroo"
    },
    "androidStatusBar": {
      "translucent": true
    }
  }
}
